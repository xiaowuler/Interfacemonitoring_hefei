<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pingchuan.weather.Dao.LegendLevelMapper">

    <select id="findAll" parameterType="String" resultType="com.pingchuan.weather.DTO.LegendLevel">
        select  * from legend_levels where type = #{id} order by endvalue;
    </select>

</mapper>

